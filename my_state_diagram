digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	start [label=start color=red fillcolor=darksalmon peripheries=2]
	menu [label=menu]
	intro [label=intro]
	helpermenu [label=helpermenu]
	input [label=input]
	web [label=web]
	mode1 [label=mode1]
	mode2 [label=mode2]
	mode3 [label=mode3]
	mode1cont [label=mode1cont]
	start -> menu [label="advance [start_to_menu]"]
	menu -> intro [label="advance [menu_to_intro]"]
	menu -> helpermenu [label="advance [menu_to_helpermenu]"]
	menu -> input [label="advance [menu_to_input]"]
	helpermenu -> mode1 [label="advance [helpermenu_to_mode1]"]
	helpermenu -> mode2 [label="advance [helpermenu_to_mode2]"]
	helpermenu -> mode3 [label="advance [helpermenu_to_mode3]"]
	helpermenu -> menu [label="advance [helpermenu_to_menu] | advance [helpermenu_to_menu]"]
	input -> web [label="advance [input_to_web]"]
	intro -> menu [label=go_back]
	web -> menu [label=go_back]
	mode1 -> helpermenu [label=go_back]
	mode2 -> helpermenu [label=go_back]
	mode3 -> helpermenu [label=go_back]
}
